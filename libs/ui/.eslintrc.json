{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "globals": {},
  "rules": {
    "react/prop-types": "off",
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "jsx"
      }
    ]
  }
}
